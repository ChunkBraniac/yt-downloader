{
    "version": 2,
    "name": "yt-downloader",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.4.0"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}