{
    "version": 2,
    "name": "yt-downloader",
    "builds": [
      {
        "src": "public/index.php",
        "use": "@vercel/php"
      },
      {
        "src": "resources/js/**",
        "use": "@vercel/static-build",
        "config": { "distDir": "public/build" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "public/index.php"
      }
    ]
  }
  