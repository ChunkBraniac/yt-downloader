{
    "version": 2,
    "name": "yt-downloader",
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.4.0"
        }
    }
}

