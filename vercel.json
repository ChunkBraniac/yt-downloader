{
    "version": 2,
    "name": "yt-downlaoder",
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}